{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.37/schema.json",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "default_workspace_padding": 0,
  "default_container_padding": 0,
  "border": true,
  "border_width": 1,
  "border_offset": 0,
  "animation": {
    "enabled": true,
    "duration": 250,
    "fps": 60,
    "style": "EaseOutExpo"
  },
  "transparency": false,
  "transparency_alpha": 245,
  "mouse_follows_focus": true,
  "hotwatch": true,
  "ensure_workspaces_on_monitor_index_0": 0,
  "monitors": [
    {
      "workspaces": [
        {
          "name": "I",
          "layout": "BSP"
        },
        {
          "name": "II", 
          "layout": "BSP"
        },
        {
          "name": "III",
          "layout": "BSP"
        },
        {
          "name": "IV",
          "layout": "BSP"
        },
        {
          "name": "V",
          "layout": "BSP"
        }
      ]
    }
  ],
  "stackbar": {
    "height": 40,
    "mode": "OnStack",
    "tabs": {
      "width": 300,
      "focused_text": {
        "r": 168,
        "g": 100,
        "b": 0
      },
      "unfocused_text": {
        "r": 255,
        "g": 255,
        "b": 255
      },
      "background": {
        "r": 16,
        "g": 16,
        "b": 20
      }
    }
  }
}